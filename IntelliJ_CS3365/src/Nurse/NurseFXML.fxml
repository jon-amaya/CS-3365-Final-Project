<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Nurse.NurseController">
   <Label layoutX="435.0" layoutY="35.0" text="Nurse Menu">
      <font>
         <Font name="Georgia" size="25.0" />
      </font>
   </Label>
   <Button fx:id="patientCheckButton" layoutX="406.0" layoutY="187.0" mnemonicParsing="false" onAction="#checkPatientMeasure" text="Check patient measurements" />
   <Button fx:id="logoutButton" layoutX="456.0" layoutY="493.0" mnemonicParsing="false" onAction="#isLogout" prefHeight="25.0" prefWidth="89.0" text="Logout" />
   <Label layoutX="383.0" layoutY="86.0" text="Please enter patient first and last name">
      <font>
         <Font size="14.0" />
      </font></Label>
   <TextField fx:id="patientFname" layoutX="406.0" layoutY="120.0" prefHeight="25.0" prefWidth="180.0" promptText="First name" />
   <TextField fx:id="patientLname" layoutX="401.0" layoutY="149.0" prefHeight="25.0" prefWidth="180.0" promptText="Last name" />
   <Label fx:id="lookupStatus" layoutX="403.0" layoutY="226.0" prefHeight="17.0" prefWidth="177.0" />
   <Label layoutX="178.0" layoutY="245.0" text="Current information">
      <font>
         <Font name="Georgia" size="20.0" />
      </font>
   </Label>
   <Label layoutX="686.0" layoutY="243.0" text="New information">
      <font>
         <Font name="Georgia" size="20.0" />
      </font>
   </Label>
   <VBox layoutX="111.0" layoutY="282.0" prefHeight="141.0" prefWidth="113.0">
      <children>
         <Label prefHeight="30.0" prefWidth="107.0" text="Weight:">
            <font>
               <Font size="15.0" />
            </font>
            <VBox.margin>
               <Insets />
            </VBox.margin>
            <padding>
               <Insets left="50.0" />
            </padding>
         </Label>
         <Label layoutX="60.0" layoutY="10.0" prefHeight="30.0" prefWidth="113.0" text="Height:">
            <font>
               <Font size="15.0" />
            </font>
            <VBox.margin>
               <Insets />
            </VBox.margin>
            <padding>
               <Insets left="52.0" />
            </padding>
         </Label>
         <Label layoutX="10.0" layoutY="31.0" prefHeight="29.0" prefWidth="113.0" text="Blood Pressure:">
            <font>
               <Font size="15.0" />
            </font>
         </Label>
         <Label layoutX="10.0" layoutY="52.0" prefHeight="35.0" prefWidth="113.0" text="Reason for visit:">
            <font>
               <Font size="15.0" />
            </font>
         </Label>
      </children>
   </VBox>
   <VBox layoutX="239.0" layoutY="275.0" prefHeight="142.0" prefWidth="121.0">
      <children>
         <TextField fx:id="currWeight" prefHeight="32.0" prefWidth="121.0" />
         <TextField fx:id="currHeight" layoutX="10.0" layoutY="10.0" prefHeight="32.0" prefWidth="121.0" />
         <TextField fx:id="currBP" layoutX="10.0" layoutY="42.0" prefHeight="32.0" prefWidth="121.0" />
         <TextField fx:id="currReason" layoutX="10.0" layoutY="74.0" prefHeight="32.0" prefWidth="121.0" />
      </children>
   </VBox>
   <VBox layoutX="662.0" layoutY="273.0" prefHeight="128.0" prefWidth="206.0">
      <children>
         <TextField fx:id="newWeight" prefHeight="32.0" prefWidth="121.0" promptText="New weight" />
         <TextField fx:id="newHeight" layoutX="10.0" layoutY="10.0" prefHeight="32.0" prefWidth="121.0" promptText="New height" />
         <TextField fx:id="newBP" layoutX="10.0" layoutY="42.0" prefHeight="32.0" prefWidth="206.0" promptText="New blood pressure" />
         <TextField fx:id="newReason" layoutX="10.0" layoutY="74.0" prefHeight="32.0" prefWidth="206.0" promptText="New reason" />
      </children>
   </VBox>
   <Button fx:id="updatePatientInfoButton" layoutX="675.0" layoutY="411.0" mnemonicParsing="false" onAction="#addPatientInfoEvent" text="Update patient measurements" />
   <Button fx:id="clearPatientInfoButton" layoutX="433.0" layoutY="270.0" mnemonicParsing="false" onAction="#clearPatientInfo" text="Clear new information" />
   <Button fx:id="clearAllButton" layoutX="452.0" layoutY="312.0" mnemonicParsing="false" onAction="#clearAllInfo" text="Clear all forms " />
   <Label fx:id="clearStatus" layoutX="388.0" layoutY="352.0" prefHeight="17.0" prefWidth="230.0" />
   <Label fx:id="dbUpdateStatus" layoutX="650.0" layoutY="445.0" prefHeight="17.0" prefWidth="224.0" />
</AnchorPane>
