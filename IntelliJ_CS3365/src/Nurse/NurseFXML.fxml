<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Nurse.NurseController">
   <children>
      <Label layoutX="81.0" layoutY="39.0" text="Nurse Menu">
         <font>
            <Font name="Georgia" size="25.0" />
         </font>
      </Label>
      <Button fx:id="patientMeasureButton" layoutX="62.0" layoutY="218.0" mnemonicParsing="false" onAction="#updatePatientMeasure" text="Update patient measurements" />
      <Button fx:id="logoutButton" layoutX="124.0" layoutY="331.0" mnemonicParsing="false" onAction="#isLogout" text="Logout" />
      <Label layoutX="49.0" layoutY="79.0" text="Please enter patient first and last name" />
      <TextField fx:id="patientFname" layoutX="78.0" layoutY="112.0" promptText="First name" />
      <TextField fx:id="patientLname" layoutX="78.0" layoutY="147.0" promptText="Last name" />
      <Label fx:id="lookupStatus" layoutX="63.0" layoutY="183.0" prefHeight="17.0" prefWidth="177.0" />
   </children>
</AnchorPane>
