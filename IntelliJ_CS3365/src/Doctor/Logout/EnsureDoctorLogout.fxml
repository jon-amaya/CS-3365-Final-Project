<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Doctor.Logout.LogoutController">
    <children>
        <Label layoutX="42.0" layoutY="40.0" text="Are you sure you want to logout?">
            <font>
                <Font name="Georgia" size="15.0" />
            </font>
        </Label>
        <Button fx:id="continueLogout" defaultButton="true" layoutX="42.0" layoutY="106.0" mnemonicParsing="false" onAction="#defsLogout" prefHeight="25.0" prefWidth="91.0" text="Continue" />
        <Button fx:id="cancelLogout" layoutX="168.0" layoutY="106.0" mnemonicParsing="false" onAction="#notLogout" prefHeight="25.0" prefWidth="91.0" text="Cancel" />
    </children>
</AnchorPane>

