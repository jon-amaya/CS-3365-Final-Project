<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="351.0" style="-fx-background-color: #3c7480;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Staff.StaffController">
    <children>
        <Button fx:id="makeAppointment" layoutX="60.0" layoutY="116.0" mnemonicParsing="false" onAction="#setMakeAppointment" prefHeight="25.0" prefWidth="242.0" text="Make Appointment" />
        <Button fx:id="changeAppointment" layoutX="60.0" layoutY="156.0" mnemonicParsing="false" onAction="#setChangeAppointment" prefHeight="25.0" prefWidth="242.0" text="Change Appointment" />
        <Button fx:id="cancelAppointment" layoutX="60.0" layoutY="194.0" mnemonicParsing="false" onAction="#setCancelAppointment" prefHeight="25.0" prefWidth="242.0" text="Cancel Appointment" />
        <Button fx:id="checkInAppointment" layoutX="60.0" layoutY="234.0" mnemonicParsing="false" onAction="#setCheckInAppointment" prefHeight="25.0" prefWidth="242.0" text="Check-In Patient" />
        <Text fill="WHITE" layoutX="89.0" layoutY="81.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Staff Menu">
            <font>
                <Font name="Arial Bold" size="36.0" />
            </font>
        </Text>
        <Button fx:id="processPayment" layoutX="60.0" layoutY="274.0" mnemonicParsing="false" onAction="#setProcessPayment" prefHeight="25.0" prefWidth="242.0" text="Process Payment" />
        <Button fx:id="logoutButton" layoutX="152.0" layoutY="317.0" mnemonicParsing="false" onAction="#isLogout" text="Log out" />
        <Text fill="WHITE" layoutX="142.0" layoutY="36.0" strokeType="OUTSIDE" strokeWidth="0.0" text="WELCOME!">
            <font>
                <Font name="Arial Bold" size="12.0" />
            </font>
        </Text>
    </children>
</AnchorPane>
